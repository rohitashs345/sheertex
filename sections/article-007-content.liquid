          min-height: 260px;
      }
  }
  
  .fall-lp-container {
    background-image: linear-gradient(to bottom, #f5ede3, #f5ede3 122%);
  }
  
  .fall-play-button {
    width: 260px;
    height: 50px;
  }
  
  .fall-play-svg {
      width: 22px;
      height: 22px;
  }
  
  .playFallVideo, .playFallVideoM  {
    position: fixed;
    top: 50%;
    bottom: 50%;
    transform: translate(0, -50%);
    z-index: 100000;
    background: rgba(0,0,0,.5);
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    visibility: hidden;
    opacity: 0;
  }
  
  .playFallVideo video, .playFallVideoM video {
    position: relative;
    max-width: 900px;
    outline: none;
  }
  .fall-video-close {
    position: absolute;
    top: 30px;
    right: 30px;
    cursor: pointer;
    filter: invert(1);
    max-width: 32px;
  }
  .fall-video-close-m {
    position: absolute;
    top: 100px;
    right: 30px;
    cursor: pointer;
    filter: invert(1);
    max-width: 24px;
  }
  .playFallVideo.active, .playFallVideoM.active {
    visibility: visible;
    opacity: 1;
  }
  @media (max-width: 991px) {
    .playFallVideo video, .playFallVideoM video {
      max-width: 90%;
    }
  }
{% endstyle %}
  
<script type="text/javascript">
    var playFallVideo = document.querySelector(".playFallVideo");
    var fallHero =  document.querySelector('.new-fall-hero-video');
    var playFallVideoM = document.querySelector(".playFallVideoM");
    var fallHeroM =  document.querySelector('.new-fall-hero-video-m');
    fallHeroM.pause();
    
    function startFallHeroPopup(){
        if(playFallVideo && fallHero) {
            fallHero.currentTime = 0;
            fallHero.autoplay = true;
            playFallVideo.classList.add("active");
        }
    }
    function stopFallHeroPopup(){
        if(playFallVideo && fallHero) {
            fallHero.currentTime = 0;
            fallHero.pause();
            playFallVideo.classList.remove("active");
        }
    }
    function startFallHeroPopupM(){
        if(playFallVideoM && fallHeroM) {
            fallHeroM.currentTime = 0;
            fallHeroM.muted = !fallHeroM.muted;
            playFallVideoM.classList.add("active");
        }
    }
    function stopFallHeroPopupM(){
        if(playFallVideoM && fallHeroM) {
            fallHeroM.currentTime = 0;
            fallHeroM.pause();
            playFallVideoM.classList.remove("active");
        }
    }
</script>

