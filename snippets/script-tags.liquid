<link rel="prefetch" href="{{ 'template.page.reviews.js' | asset_url }}" as="script">
{%- endif -%} {%- if template == 'page.sheertex-our-story' -%}
<script type="text/javascript" src="{{ 'template.page.sheertex-our-story.js' | asset_url }}" defer="defer"></script>
{%- else -%}
<link rel="prefetch" href="{{ 'template.page.sheertex-our-story.js' | asset_url }}" as="script">
{%- endif -%} {%- if template == 'page.thigh-highs' -%}
<script type="text/javascript" src="{{ 'template.page.thigh-highs.js' | asset_url }}" defer="defer"></script>
{%- else -%}
<link rel="prefetch" href="{{ 'template.page.thigh-highs.js' | asset_url }}" as="script">
{%- endif -%} {%- if template contains 'product' -%}
<script type="text/javascript" src="{{ 'template.product.js' | asset_url }}" defer="defer"></script>
