
{%- if layout == 'checkout' -%}
<script type="text/javascript" src="{{ 'layout.checkout.js' | asset_url }}" defer="defer"></script>
{%- else -%}
<link rel="prefetch" href="{{ 'layout.checkout.js' | asset_url }}" as="script">
{%- endif -%} {%- if layout == 'theme' -%}
<script type="text/javascript" src="{{ 'layout.theme.js' | asset_url }}" defer="defer"></script>
