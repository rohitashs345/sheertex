        padding-bottom: 10rem;
    }
}

@media (max-width: 640px) {
    .fall-page-hero {
        min-height: 65vh;
    }
    .fall-quote-content {
        min-height: 260px;
    }
}

.fall-lp-container {
  background-image: linear-gradient(to bottom, #f5ede3, #f5ede3 122%);
}

.fall-play-button {
  width: 260px;
  height: 50px;
}

.fall-play-svg {
    width: 22px;
    height: 22px;
}

.playFallVideoFr, .playFallVideoFrM  {
  position: fixed;
  top: 50%;
  bottom: 50%;
  transform: translate(0, -50%);
  z-index: 100000;
  background: rgba(0,0,0,.5);
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  visibility: hidden;
  opacity: 0;
}

.playFallVideoFr video, .playFallVideoFrM video {
  position: relative;
  max-width: 900px;
  outline: none;
}
.fall-video-close {
  position: absolute;
  top: 30px;
  right: 30px;
  cursor: pointer;
  filter: invert(1);
  max-width: 32px;
}
.fall-video-close-m {
  position: absolute;
  top: 100px;
  right: 30px;
  cursor: pointer;
  filter: invert(1);
  max-width: 24px;
}
.playFallVideoFr.active, .playFallVideoFrM.active {
  visibility: visible;
  opacity: 1;
}
@media (max-width: 991px) {
  .playFallVideoFr video, .playFallVideoFrM video {
    max-width: 90%;
  }
}
{% endstyle %}

<script type="text/javascript">
var playFallVideoFr = document.querySelector(".playFallVideoFr");
var fallHeroFr =  document.querySelector('.new-fall-hero-video-fr');
var playFallVideoFrM = document.querySelector(".playFallVideoFrM");
var fallHeroFrM =  document.querySelector('.new-fall-hero-video-fr-m');
fallHeroFrM.pause();

function startFallHeroFrPopup(){
    if(playFallVideoFr && fallHeroFr) {
    fallHeroFr.currentTime = 0;
    fallHeroFr.autoplay = true;
    playFallVideoFr.classList.add("active");
    }
}
function stopFallHeroFrPopup(){
    if(playFallVideoFr && fallHeroFr) {
    fallHeroFr.currentTime = 0;
    fallHeroFr.pause();
    playFallVideoFr.classList.remove("active");
    }
}
function startFallHeroFrPopupM(){
    if(playFallVideoFrM && fallHeroFrM) {
    fallHeroFrM.currentTime = 0;
    fallHeroFrM.muted = !fallHeroFrM.muted;
    playFallVideoFrM.classList.add("active");
    }
}
function stopFallHeroFrPopupM(){
    if(playFallVideoFrM && fallHeroFrM) {
    fallHeroFrM.currentTime = 0;
    fallHeroFrM.pause();
    playFallVideoFrM.classList.remove("active");
    }
}
</script>